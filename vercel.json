{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/admin",
      "dest": "/private/index.html"
    },
    {
      "src": "/private/(.*)",
      "dest": "/private/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}