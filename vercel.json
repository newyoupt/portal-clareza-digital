{
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/private/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/private/index.html"
    }
  ],
  "routes": [
    {
      "src": "/private/(.*)",
      "dest": "/private/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}